- load:
  - "{{metadata_proxy_frontend_source}}"
- select
- xslt:
    stylesheet: "{{metadata_basedir}}/transform.xslt"
- finalize:
    cacheDuration: P7D
    validUntil: P14D
- sign:
    key: "{{pyff_cert_dir}}/signing.key"
    cert: "{{pyff_cert_dir}}/signing.crt"
- publish: "{{metadata_target_dir}}/{{metadata_proxy_frontend_file}}.new"
- stats
